<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pknu.main.dao.MemberDao">

	
	<insert id="insertMember" parameterType="com.pknu.main.dto.MemberDto">
		INSERT INTO DOR_MEMBER
		VALUES(#{id}, #{password}, #{name}, #{grade}, TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS'))
	</insert>
	
	<select id="selectPassword" parameterType="string" resultType="string">
		SELECT PW
		FROM DOR_MEMBER
		WHERE ID = #{id}
	</select>
	
</mapper>