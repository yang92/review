<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pknu.member.dao.MemberDao">

	
	<insert id="insertMember" parameterType="com.pknu.member.dto.MemberDto">
		INSERT INTO MEMBER
		VALUES(member_seq.nextval, #{id}, #{pw}, #{name}, #{email}, #{sex}, #{birth}, sysdate, 0, #{memo})
	</insert>
	
	<select id="selectPassword" parameterType="string" resultType="string">
		SELECT MEMBER_PW
		FROM MEMBER
		WHERE MEMBER_ID = #{id}
	</select>
	
</mapper>